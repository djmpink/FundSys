<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.no7player.fundsys.po">
    <class name="FundTradeData" >
        <id name="id" >
            <generator class="identity"/>
        </id>
        <property name="fundCode"></property>
        <property name="fundName"></property>
        <property name="purchaseCredit"></property>
        <property name="purchaseShare"></property>
        <property name="purchaseNetWorth"></property>
        <property name="miniNetWorth"></property>
        <property name="purchaseFee"></property>
        <property name="purchaseDate"></property>
        <property name="saleShare"></property>
        <property name="saleNetWorth"></property>
        <property name="earnings"></property>
        <property name="profit"></property>
        <property name="saleFee"></property>
        <property name="saleDate"></property>
        <property name="userId"></property>
    </class>
</hibernate-mapping>
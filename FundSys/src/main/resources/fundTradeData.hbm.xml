<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.no7player.fundsys.po">
    <class name="FundTradeData" >
        <id name="id" >
            <generator class="identity"/>
        </id>
        <property name="code"></property>
        <property name="name"></property>
        <property name="purchase"></property>
        <property name="share"></property>
        <property name="purchaseFee"></property>
        <property name="purchaseDate"></property>
        <property name="saleRate"></property>
        <property name="userId"></property>
        <property name="purchaseNetWorth"></property>
    </class>
</hibernate-mapping>